<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>NameLength</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(Name)  &gt; 48</errorConditionFormula>
    <errorMessage>The building project name or city name is too long.</errorMessage>
</ValidationRule>
